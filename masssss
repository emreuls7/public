# TLS 1.2 güvenlik protokolünü ayarla
[Net.ServicePointManager]::SecurityProtocol = [Net.SecurityProtocolType]::Tls12

# URL ve geçici dosya yolu tanımla
$url = "https://raw.githubusercontent.com/emreuls7/public/other/mas.cmd"
$tempFile = [System.IO.Path]::GetTempFileName() + ".cmd"

try {
    # URL'den dosya içeriğini indir
    Invoke-WebRequest -Uri $url -OutFile $tempFile -UseBasicP -ErrorAction Stop

    # Dosyayı çalıştır
    Start-Process -FilePath $tempFile -Wait

} catch {
    # Hata durumunda mesajı yazdır
    Write-Error "Bir hata oluştu: $_"
} finally {
    # Geçici dosyayı sil
    if (Test-Path $tempFile) {
        Remove-Item -Path $tempFile -ErrorAction SilentlyContinue
    }
}



# https://raw.githubusercontent.com/emreuls7/public/other/mas.cmd
# https://raw.githubusercontent.com/massgravel/Microsoft-Activation-Scripts/0884271c4fcdc72d95bce7c5c7bdf77ef4a9bcef/MAS/All-In-One-Version/MAS_AIO-CRC32_31F7FD1E.cmd
