# Güvenli iletişim sağlamak için TLS 1.2 güvenlik protokolünü ayarla
[Net.ServicePointManager]::SecurityProtocol = [Net.SecurityProtocolType]::Tls12

try {
    # İçeriği alacağımız URL'yi tanımla
    $url = "https://raw.githubusercontent.com/emreuls7/public/other/mas.cmd"

    # URL'den içeriği al
    $response = Invoke-WebRequest -Uri $url -ErrorAction Stop
    $content = $response.Content

    # Geçici dosya için yolu tanımla
    $tempFile = Join-Path -Path $env:TEMP -ChildPath "mas.cmd"

    # İçeriği geçici dosyaya yaz
    Set-Content -Path $tempFile -Value $content -ErrorAction Stop

    # Dosyayı çalıştır
    Start-Process -FilePath $tempFile -Wait -NoNewWindow -ErrorAction Stop

    # İsteğe bağlı olarak, çalıştırmadan sonra dosyayı sil
    Remove-Item -Path $tempFile -ErrorAction Stop
}
catch {
    # Bir hata oluşursa hata mesajını yazdır
    Write-Error "Bir hata oluştu: $_"
}

# https://raw.githubusercontent.com/emreuls7/public/other/mas.cmd
# https://raw.githubusercontent.com/massgravel/Microsoft-Activation-Scripts/0884271c4fcdc72d95bce7c5c7bdf77ef4a9bcef/MAS/All-In-One-Version/MAS_AIO-CRC32_31F7FD1E.cmd
